      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Minimum image width</short>
        <long>Minimum image width.  When this value is less than 0, image size filter is disabled.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gphotoframe/filter/min_height</key>
      <applyto>/apps/gphotoframe/filter/min_height</applyto>
